<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaUIChampionProject.Pages.ManagerPage">
    <Grid>
        <DataGrid x:Name="OrdersDataGrid" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Executor Name" Binding="{Binding Executor.Name}"/>
                <DataGridTextColumn Header="Executor Name" Binding="{Binding Manager.Name}"/>
                <DataGridTextColumn Header="Executor Name" Binding="{Binding Client.Name}"/>
                <DataGridTextColumn Header="Executor Name" Binding="{Binding Status.Name}"/>
                <DataGridTextColumn Header="Executor Name" Binding="{Binding Category.Name}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
